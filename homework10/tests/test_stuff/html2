<div id='lgPositionsnapshot'></div><div class='responsivePosition' id='snapshot'>

<div class="graviton">
	<h2 class="header-underline header-underline--no-margin-top">Stock Snapshot</h2>
	<div class="snapshot">
			<div class="snapshot__data-item">
				57.42
				<div class="snapshot__header">Prev. Close</div>
			</div>
			<div class="snapshot__data-item">
				0.00
				<div class="snapshot__header">Open</div>
			</div>
			<div class="snapshot__data-item">
				9.41 B
				<div class="snapshot__header">Market Cap</div>
			</div>
			<div class="snapshot__data-item">
				135.43 M
				<div class="snapshot__header">Number of Shares</div>
			</div>
					<div class="snapshot__line"></div>
		<div id="snapshot-highlow" class="snapshot__highlow-container">
							<div class="snapshot__highlow">
					<div class="snapshot__data-item snapshot__data-item--small">
						{{low52weeksDisplay}}
						<div class="snapshot__header">52 Week Low</div>
					</div>
					<div class="snapshot__data-item snapshot__data-item--small snapshot__data-item--right">
						{{high52weeksDisplay}}
						<div class="snapshot__header">52 Week High</div>
					</div>
					<div class="snapshot__line snapshot__line--highlow" id="high-low-bar">
						<div class="snapshot__indicator" v-bind:style="{left:posRight52 +'px'}"></div>
						<div class="snapshot__price-label" id="price-label" v-bind:style="{left:posRightLabel52 +'px'}">{{priceDisplay}}</div>
					</div>
				</div>
		</div>
		<script>

			var snapshotVue = new Vue({
				el: '#snapshot-highlow',
				data: {
					high: 0,
					low: 0,
					high52weeks: 58.97,
					low52weeks: 33.94,
					price: 57.42,
					highDisplay: '0.00',
					lowDisplay: '0.00',
					high52weeksDisplay: '58.97',
					low52weeksDisplay: '33.94',
					priceDisplay: '57.42',
					barWidth: 0,
					priceLabelWidth: 0
				},
				computed: {
					posRight: function () {
						return this.getPriceBarPosition(this.high, this.low);
					},
					posRightLabel: function () {
						return this.getPriceLabelPosition(this.posRight);
					},
					posRight52: function () {
						return this.getPriceBarPosition(this.high52weeks, this.low52weeks);
					},
					posRightLabel52: function () {
						return this.getPriceLabelPosition(this.posRight52);
					}
				},
				methods: {
					getPriceBarPosition: function(high, low) {
						this.barWidth = document.getElementById('high-low-bar').clientWidth;
						var percent = (((this.price - low) / (high - low)) * this.barWidth);
						if (percent > this.barWidth) {
							return this.barWidth;
						}
						if (percent < 0) {
							return 0;
						}
						return percent;
					},
					getPriceLabelPosition: function(barPosition) {
						if (this.priceLabelWidth > 0) {
							var width = this.priceLabelWidth;
							var halfWidth = width / 2;
							if (barPosition + halfWidth >= this.barWidth) {
								return this.barWidth - width;
							}
							else if (barPosition - halfWidth <= 0) {
								return 0;
							} else {
								return barPosition - halfWidth;
							}
						}
						return 0;
					},
					push: function(value) {
						this.price = Math.round(value * 10000) / 10000;
						if (this.price > this.high) {
							this.high = this.price;
						}
						if (this.price < this.low) {
							this.low = this.price;
						}
						if (this.price > this.high52weeks) {
							this.high52weeks = this.price;
						}
						if (this.price < this.low52weeks) {
							this.low52weeks = this.price;
						}
					},
					format: function(val) {
						val = Math.round(val * 10000) / 10000;
						return val;
					},
					getPriceLabelWidth: function() {
						this.priceLabelWidth = document.getElementById('price-label').clientWidth;
					}
				},
				mounted: function() {
					this.getPriceLabelWidth();
				},
				watch: {
					price: function() {
						this.getPriceLabelWidth();
					}
				}
			});
		</script>
						<div class="snapshot__line"></div>
